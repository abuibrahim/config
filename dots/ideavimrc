let mapleader=" "
set clipboard+=unnamed
set incsearch
set scrolloff=8
set surround
# set relativenumber

nmap <Leader>rr  :action RenameElement<CR>
nmap <Leader>ref :action ExtractFunction<CR>

nmap <Leader>ns  :action GotoSuperMethod<CR>
nmap <Leader>ni  :action GotoImplementation<CR>

nmap <Leader>ij  :action EditorStartNewLine<CR>
nmap <Leader>ik  :action EditorStartNewLineBefore<CR>

nmap <Leader><Leader> :action AceJumpAction<CR>
imap fd <ESC>
