export PURE_PROMPT_SYMBOL=Î»
source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"

source ~/.profile

unalias gcf
function gcf() {
    local user=$1
    local repo=$2
    git clone https://github.com/$user/$repo.git
    pushd $repo
    git remote add fork git@github.com:matklad/$repo.git
    git fetch fork
    popd
}
